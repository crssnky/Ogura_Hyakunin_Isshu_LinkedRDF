    <OguraHyakuninIsshu:LowerPhrase_Kana rdf:datatype="https://www.w3.org/TR/xmlschema11-2/#string">おきまどはせるしらぎくのはな</OguraHyakuninIsshu:LowerPhrase_Kana>
    <rdf:type rdf:resource="https://sparql.crssnky.xyz/OguraHyakuninIsshu/URIs/Ogura_Hyakunin_Isshu_schema.ttl#Phrase"/>
